      },
      "package": "com.royanurag10.Notification_app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "909a3586-09a9-43c9-8a50-8e7aa3a93e25"
      }
